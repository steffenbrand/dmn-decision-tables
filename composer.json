{
    "name": "steffenbrand/dmn-decision-tables",
    "description": "PHP library to programmatically create DMN decision tables.",
    "keywords": [
        "DMN",
        "BPMN",
        "Decision Table",
        "Camunda"
    ],
    "homepage": "https://github.com/steffenbrand/dmn-decision-tables",
    "license": "MIT",
    "type": "library",
    "version": "1.0.0",
    "authors": [
        {
            "name": "Steffen Brand",
            "email": "s.brand@steffenbrand.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7"
    },
    "autoload": {
        "psr-4": {
            "SteffenBrand\\DmnDecisionTables\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SteffenBrand\\DmnDecisionTables\\Test\\": "test/"
        }
    },
    "config": {
        "bin-dir": "bin"
    }
}
